            android:icon="@drawable/logo"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.wenmingvs.rainymood.service.PlayService">
           
            
        </service>
        <receiver android:name="com.wenmingvs.rainymood.receiver.AlarmReceiver"></receiver>
        <service android:name="com.wenmingvs.rainymood.service.CounterService"></service>
    </application>

</manifest>
